{
  "container": {
    "type": "DOCKER",
      "docker": {
        "image": "{{.MesosDockerImage}}"
      }
  },
    "id": "{{.SlaveAppId}}",
    "instances": "{{.SlaveCount}}",
    "cpus": "{{.SlaveCpus}}",
    "mem": "{{.SlaveMem}}",
    "ports": [0],
    "uris": [],
    "cmd": "mesos-slave --port=$PORT0 --master={{.ZookeeperUrl}} --work_dir=. {{.SlaveFlags}}"
}
