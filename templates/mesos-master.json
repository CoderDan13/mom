{
  "container": {
    "type": "DOCKER",
      "docker": {
        "image": "{{.MesosDockerImage}}"
      }
  },
    "id": "{{.MasterAppId}}",
    "instances": "{{.MasterCount}}",
    "cpus": "{{.MasterCpus}}",
    "mem": "{{.MasterMem}}",
    "ports": [0],
    "uris": [],
    "cmd": "mesos-master --port=$PORT0 --zk={{.ZookeeperUrl}} --work_dir=. --quorum={{.QuorumSize}} {{.MasterFlags}}"
}
